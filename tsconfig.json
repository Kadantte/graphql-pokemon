{
  "extends": "@favware/ts-config",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es2016",
      "esnext.asynciterable"
    ],
    "outDir": "dist",
    "rootDir": "src",
    "target": "es2017",
    "strictPropertyInitialization": false,
    "resolveJsonModule": true
  },
  "include": [
    "src"
  ]
}